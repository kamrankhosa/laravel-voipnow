{
    "name": "kamrankhosa/laravel-voipnow",
    "description": "A laravel package to interact with voipnow API",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/kamrankhosa/laravel-voipnow",
    "keywords": [
        "4psa",
        "laravel-voipnow",
        "voipnow",
        "voipnow-laravel-package",
        "kamrankhosa-voipnow"
    ],
    "autoload": {
        "psr-4": {
            "Kamrankhosa\\LaravelVoipnow\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kamran Haider",
            "email": "kamrankhosa40@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^10.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Kamrankhosa\\LaravelVoipnow\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kamrankhosa\\LaravelVoipnow\\VoipNowServiceProvider"
            ],
            "aliases": {
                "VoipNow": "Kamrankhosa\\LaravelVoipnow\\VoipNowFacade"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
